<div id="forms">
  <header>Add New Alumni Dog</header>
  <br>
  <%= form_for @testimonial, html: { multipart: true }, remote: true do |f| %>
    <div>
      <%= f.label :name %>
      <br>
      <%= f.text_field :name, required: 'required' %>
      <br><br>
      <%= f.label :image %>
      <%= f.file_field :image %>
      <br>
      <%= f.submit class: 'btn btn-success' %>
    </div>
  <% end %>
</div>
